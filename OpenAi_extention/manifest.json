
{
  "manifest_version": 2,
  "name": "OpenAI Text Extractor",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "https://api.openai.com/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}


